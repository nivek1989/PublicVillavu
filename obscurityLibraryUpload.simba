  function glModelArray.toTPA():tPointArray;
    var
      selfIndex:cardinal=0;
    begin
      setLength(result,self.indexes());
      for selfIndex to self.maxIndex() do
        begin
          result[selfIndex].x:=self[selfIndex].x;
          result[selfIndex].y:=self[selfIndex].y;
        end;
    end;

